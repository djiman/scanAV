pipeline {
    agent {
      node {
        label "master"
      } 
    }

    stages {
      stage('fetch_latest_code') {
        steps {
          git credentialsId: 'GitHubAccess', url: 'https://github.com/djiman/scanAV.git'      
        }
      }

      stage('TF Init&Plan') {
        steps {
          sh 'terraform init'
          sh 'terraform plan -var='s3_targets=[ "s3-bucket-quarantine" ]''
        }      
      }
    } 
  }